{
  "name": "FreezeFrame",
  "version": "0.9.5",
  "author": "Joseph Liversedge <jliverse@vt.edu>",
  "contributors": [
    "Dr. M. A. Bell <mabell@vt.edu>",
    "L. Patton <lapatton@vt.edu>"
  ],
  "description":
    "For researchers of infant cognition and human development, Freeze Frame is an interactive computer application that provides experimental control of visual cues, animation and stimuli for scientific trials of early visual inhibition.",
  "main": "index.js",
  "scripts": {
    "start":
      "cross-env NODE_ENV=development webpack-dev-server --hot --inline --progress",
    "start:app": "electron-webpack dev",
    "jest": "jest --no-cache --watch",
    "build":
      "cross-env NODE_ENV=production webpack --progress --profile --colors",
    "build:app": "electron-webpack && electron-builder",
    "build:package":
      "npm run build:app -- --dir -c.compression=store -c.mac.identity=null",
    "build:browser":
      "cross-env NODE_ENV=production webpack --hide-modules --config webpack.browser.babel.js",
    "build:start":
      "cross-env NODE_ENV=production webpack-dev-server --hot --inline --progress",
    "test": "jest --no-cache --verbose",
    "test:browser":
      "cross-env NODE_ENV=test webpack-dev-server --hot --inline --progress --config webpack.browser.babel.js",
    "release": "npm run build",
    "stats":
      "cross-env NODE_ENV=production webpack --json | webpack-bundle-size-analyzer",
    "lint:css": "immutable-css public/*.css --strict",
    "lint:js": "eslint --quiet **/*.js",
    "prebuild": "npm test",
    "prebuild:app": "npm run build",
    "prelint:css": "npm run build"
  },
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/BookAndBranch/freeze-frame.git"
  },
  "bugs": {
    "url": "https://github.com/BookAndBranch/freeze-frame/issues"
  },
  "homepage": "https://github.com/BookAndBranch/freeze-frame",
  "build": {
    "appId": "edu.vt.phys.freezeframe",
    "files": ["public/**/*", "package.json"],
    "publish": null,
    "mac": {
      "target": "pkg"
    },
    "win": {
      "target": "zip"
    }
  },
  "jest": {
    "collectCoverageFrom": ["src/**/*.js"],
    "testPathIgnorePatterns": [
      "<rootDir>/src/.*/__browser__",
      "<rootDir>/test"
    ],
    "testEnvironment": "jsdom",
    "modulePathIgnorePatterns": [
      "/tmp",
      "<rootDir>/node_modules/",
      "<rootDir>/src/.*/__mocks__"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|less)$": "identity-obj-proxy",
      "\\.(jpg|png|gif|mp3)$": "<rootDir>/__mocks__/fileMock.js",
      "^lib(.*)$": "<rootDir>/src/lib$1"
    },
    "transform": {
      "^.+\\.js$": "babel-jest",
      "\\.(svg)$": "<rootDir>/__mocks__/fileTransformer.js"
    }
  },
  "electronWebpack": {
    "title": true,
    "main": {
      "sourceDirectory": "app/main"
    },
    "renderer": {
      "dll": ["preact"],
      "sourceDirectory": "app/renderer"
    }
  },
  "dependencies": {
    "lodash": "^4.17.4",
    "preact": "^8.1.0",
    "source-map-support": "^0.4.17"
  },
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.2",
    "babel-minify": "^0.2.0",
    "babel-minify-webpack-plugin": "^0.2.0",
    "babel-plugin-dynamic-import-node": "^1.0.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "babel-plugin-transform-function-bind": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-minify": "^0.2.0",
    "bytes": "^3.0.0",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.4",
    "dlv": "^1.1.0",
    "electron": "^1.7.9",
    "electron-builder": "^19.26.0",
    "electron-packager": "^9.1.0",
    "electron-webpack": "^1.1.0",
    "eslint": "^4.4.1",
    "eslint-config-prettier": "^2.3.0",
    "eslint-plugin-prettier": "^2.2.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "file-saver": "^1.3.3",
    "fsevents": "^1.1.2",
    "happypack": "^4.0.0-beta.5",
    "html-webpack-plugin": "^2.28.0",
    "identity-obj-proxy": "^3.0.0",
    "immutable-css-cli": "^1.1.1",
    "jest": "^20.0.4",
    "json2csv": "^3.11.4",
    "less": "^2.7.2",
    "less-loader": "^4.0.5",
    "linkstate": "^1.1.0",
    "normalize.css": "^7.0.0",
    "postcss-clean": "^1.0.3",
    "postcss-import": "^10.0.0",
    "postcss-loader": "^2.0.6",
    "preact-compat": "^3.17.0",
    "preact-render-spy": "^1.0.0-rc.8",
    "preact-render-to-string": "^3.6.3",
    "preact-router": "^2.5.7",
    "prettier": "^1.5.3",
    "raw-loader": "^0.5.1",
    "resolve-url-loader": "^2.1.0",
    "revalidator": "^0.3.1",
    "simple-mock": "^0.8.0",
    "spectre.css": "https://github.com/picturepan2/spectre.git#less",
    "style-loader": "^0.18.2",
    "tap-console-parser": "^1.0.4",
    "tape": "^4.8.0",
    "tape-catch": "^1.0.6",
    "url-loader": "^0.5.9",
    "webpack": "^3.5.5",
    "webpack-bundle-size-analyzer": "^2.7.0",
    "webpack-dev-server": "^2.5.0",
    "whatwg-fetch": "^2.0.3",
    "xml-light": "^1.0.8",
    "xml2js": "^0.4.19"
  }
}
